sampler gShadowMap;struct PS_OUTPUT{	float4 Shadow	: COLOR0;	float4 BlurCoef	: COLOR1;};PS_OUTPUT PSMain (float4 _PosProj : TEXCOORD0, float2 ZW : TEXCOORD1){	PS_OUTPUT Output;		float gZBias = 0.0065f;	_PosProj.xyz /= _PosProj.w;	_PosProj.xy = (_PosProj.xy + (1.f).xx) / (2.f).xx;	_PosProj.y = 1.f - _PosProj.y;	float Depth = tex2D(gShadowMap, _PosProj.xy);		if ( (_PosProj.x >= 0.f && _PosProj.x <= 1.f) &&		 (_PosProj.y >= 0.f && _PosProj.y <= 1.f) )	{		float ZDiff = abs(_PosProj.z - Depth);		if ( ZDiff >= gZBias )		{			Output.Shadow	= float4( (0.f).xxx, 1.f );			Output.BlurCoef	= float4( ZDiff.xxx * ( 1.f - ZW.x / ZW.y), 1.f );			return Output;		}	}	Output.Shadow	= float4( (1.f).xxx, 1.f );	Output.BlurCoef	= float4( (0.f).xxx, 1.f );	return Output;}